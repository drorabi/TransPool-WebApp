<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>SetMatchServlet</servlet-name>
        <servlet-class>chat.servlets.SetMatchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>chat.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MakeMatchServlet</servlet-name>
        <servlet-class>chat.servlets.MakeMatchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccordionDataServlet</servlet-name>
        <servlet-class>chat.servlets.AccordionDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FeedbackAccordionServlet</servlet-name>
        <servlet-class>chat.servlets.FeedBackAccordionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MapsListServlet</servlet-name>
        <servlet-class>chat.servlets.MapsListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MapServlet</servlet-name>
        <servlet-class>chat.servlets.MapServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>chat.servlets.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BalanceServlet</servlet-name>
        <servlet-class>chat.servlets.BalanceServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addStationServlet</servlet-name>
        <servlet-class>chat.servlets.addStationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addFeedbackServlet</servlet-name>
        <servlet-class>chat.servlets.addFeedbackServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlertServlet</servlet-name>
        <servlet-class>chat.servlets.AlertServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarkStationsServlet</servlet-name>
        <servlet-class>chat.servlets.MarkStationsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarkRouteServlet</servlet-name>
        <servlet-class>chat.servlets.MarkRouteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RestartAllColorsServlet</servlet-name>
        <servlet-class>chat.servlets.RestartAllColorsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChatServlet</servlet-name>
        <servlet-class>chat.servlets.ChatServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsersListServlet</servlet-name>
        <servlet-class>chat.servlets.UsersListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UsersListServlet</servlet-name>
        <url-pattern>/userslist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChatServlet</servlet-name>
        <url-pattern>/chat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlertServlet</servlet-name>
        <url-pattern>/Alert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BalanceServlet</servlet-name>
        <url-pattern>/Balance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/pages/signin/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MapsListServlet</servlet-name>
        <url-pattern>/MapsData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/pages/profile/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BalanceServlet</servlet-name>
        <url-pattern>/updateMoney</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MapServlet</servlet-name>
        <url-pattern>/map</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccordionDataServlet</servlet-name>
        <url-pattern>/accordion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeedbackAccordionServlet</servlet-name>
        <url-pattern>/feedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addStationServlet</servlet-name>
        <url-pattern>/addStation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MakeMatchServlet</servlet-name>
        <url-pattern>/makeMatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetMatchServlet</servlet-name>
        <url-pattern>/setMatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addFeedbackServlet</servlet-name>
        <url-pattern>/addFeedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RestartAllColorsServlet</servlet-name>
        <url-pattern>/restartAllColors</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarkRouteServlet</servlet-name>
        <url-pattern>/markRoute</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarkStationsServlet</servlet-name>
        <url-pattern>/markStations</url-pattern>
    </servlet-mapping>
</web-app>
